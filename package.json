{
  "name": "koa-starter",
  "version": "0.0.1",
  "description": "Just a small Koa starter using TypeScript",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:server": "",
    "build:client": "webpack --progress --mode production",
    "start:watch": "nodemon --watch 'src/server/**/*' -e ts,tsx,js,jsx --exec ts-node ./src/server/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "knex": "knex",
    "knex:migrate": "knex migrate:latest",
    "knex:seed": "knex seed:run"
  },
  "author": "Henrik Hedlund <henrik@hedlund.im>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^7.0.0",
    "knex": "^0.16.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-passport": "^4.1.1",
    "koa-redis": "^3.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "passport-local": "^1.0.0",
    "pg": "^7.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/knex": "^0.16.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-redis": "^3.0.2",
    "@types/koa-router": "^7.0.40",
    "@types/koa-session": "^5.10.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.8.11",
    "@types/react-dom": "^16.8.3",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.2.2",
    "nodemon": "^1.18.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
